# Change to directory where compile_commands.json is
compilation_database_dir: build
# Change to directory where diagram should be written
query_diver
output_directory: docs/diagrams
diagrams:
  example_class_diagram:
    type: class
    glob:
      - main.cpp
    using_namespace:
      - myproject
    include:
      namespaces:
        - myproject
    exclude:
      namespaces:
        - myproject::detail
